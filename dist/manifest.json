{
  "manifest_version": 2,
  "name": "ConnecTouchExtension",
  "description": "extension that notify ConnecTouch Events",
  "version": "0.1.3",
  "icons": {
    "48": "/icons/icon.png"
  },
  "permissions": [
    "notifications",
    "storage",
    "*://connectouch.org/*",
    "*://192.168.0.200/*",
    "webRequest"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "option.html"
  },
  "applications": {
    "gecko": {
      "id": "hykwtakumin@gmail.com013",
      "update_url":"https://raw.githubusercontent.com/Hykwtakumin/ConnecTouchExtension/master/updates.json",
      "strict_min_version": "66.0"
    }
  }
}