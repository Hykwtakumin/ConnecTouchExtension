{
  "name": "ConnecTouchExtension",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "mkdir ./dist/pages ./dist/icons ./dist/styles",
    "clean": "rm -rf dist/*",
    "tsc": "tsc",
    "copy": "cp ./manifest.json ./dist && cp -r ./src/icons ./dist/icons",
    "browserify-bg": "browserify ./dist/background.js -o ./dist/background.js",
    "browserify-cs": "browserify ./dist/scripts/content.js -o ./dist/scripts/content.js",
    "browserify-op": "browserify ./dist/scripts/option.js -o ./dist/scripts/option.js",
    "browserify": "npm run browserify-bg && npm run browserify-cs && npm run browserify-op",
    "start": "web-ext run --source-dir ./dist",
    "build": "npm run clean && npm run copy && npm run tsc && npm run browserify-bg",
    "android": "web-ext run --source-dir ./dist --target=firefox-android --android-device=2eda80a4 --firefox-apk=org.mozilla.fennec_aurora",
    "dev": "npm run build && npm run android",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "hykwtakumin <hykwtakumin@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "browserify": "^16.2.3",
    "fsbx": "^2.1.0-beta.4",
    "fuse-box": "^3.7.0",
    "fuse-box-typechecker": "^2.11.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3",
    "uglify-es": "^3.3.9",
    "uglify-js": "^3.4.9",
    "web-ext": "^2.9.3",
    "web-ext-types": "^3.1.0"
  }
}
