{
  "manifest_version": 2,
  "name": "ConnecTouchExtension",
  "description": "extension that notify ConnecTouch Events",
  "version": "0.1.0",
  "permissions": [
    "notifications",
    "storage",
    "*://connectouch.org/*",
    "*://192.168.0.200/*",
    "webRequest"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}